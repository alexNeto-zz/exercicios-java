package lista_1;

public class Main {

	public static void main(String[] args) {

		System.out.println("Escolha um Exercicio:");
		java.util.Scanner input = new java.util.Scanner(System.in);
		int escolha = input.nextInt();
		switch (escolha) {
		case 1:
			Exercicio_1.exercicioI();
			break;
		case 2:
			Exercicio_2.exercicioI();
			break;
		case 3:
			Exercicio_3.exercicioI();
			break;
		case 4:
			Exercicio_4.exercicioI();
			break;
		case 5:
			Exercicio_5.exercicioI();
			break;
		case 6:
			Exercicio_6.exercicioI();
			break;
		case 7:
			Exercicio_7.exercicioI();
			break;
		case 8:
			Exercicio_8.exercicioI();
			break;
		case 9:
			Exercicio_1.exercicioI();
			Exercicio_2.exercicioI();
			Exercicio_3.exercicioI();
			Exercicio_4.exercicioI();
			Exercicio_5.exercicioI();
			Exercicio_6.exercicioI();
			Exercicio_7.exercicioI();
			Exercicio_8.exercicioI();
			break;

		default:
			break;
		}

	}

}
