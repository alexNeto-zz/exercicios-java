package lista_3;

public class Exercicio_5 {

	public static void exercicio() {

		int[] numeros = new int[1000];
		int j = 0;
		for (int i = 2; i < numeros.length; i++) {
			if (primos(i)) {
				numeros[j++] = i;
				System.out.println(numeros[j]);
			}
		}

	}

	public static boolean primos(int i) {
		for (int j = 2; j < i - 1; j++) {
			if (i % j == 0) {
				return false;
			}
		}
		return true;
	}
}
