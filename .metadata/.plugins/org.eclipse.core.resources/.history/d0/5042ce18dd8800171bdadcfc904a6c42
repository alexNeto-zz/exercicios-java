package lista_3;

public class Exercicio_5 {

	public static void exercicio() {

		int[] numeros = new int[1000];
		for (int i = 0; i < numeros.length; i++) {
			if (primos(i)) {
				numeros[i] = i;
			}
		}

	}

	public static boolean primos(int i) {
		boolean flag = false;
		for (int j = 2; j < i - 1; j++) {
			flag = false;
			for (int i = numeros.length; i > 1; i--) {
				if (numeros[j] % i == 0) {
					flag = true;
				}
			}
			if (flag) {
				System.out.printf("%d ", numeros[j]);
			}

		}
	}
}
